{
  "manifest_version": 2,
  "name": "FP Glossary",
  "description": "Learn functional programming, one tab at a time.",
  "version": "2",
  "author": "Olle Linderos",
  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "New tab"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["activeTab"]
}
